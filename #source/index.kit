<!DOCTYPE html>
<html lang="ru">

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="css/style.css">
	<link rel="stylesheet" href="css/select.css">
	<link rel="shortcut icon" href="favicon.ico">
	<link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
	<title>Wooder</title>
</head>

<body>
	<div class="wrapper">
		<!-- @import "header.kit" -->
		<div class="content">
			<section class="main">
				<div class="main__photos">
					<div class="main__slider swiper-container">
						<div class="main__wrapper swiper-wrapper">
							<div class="main__slide swiper-slide">
								<img src="img/main/01.png" alt="">
							</div>
							<div class="main__slide swiper-slide">
								<img src="img/main/01.png" alt="">
							</div>
							<div class="main__slide swiper-slide">
								<img src="img/main/01.png" alt="">
							</div>
							<div class="main__slide swiper-slide">
								<img src="img/main/01.png" alt="">
							</div>
						</div>
						<div class="main__navigation">
							<div class="main__wrap">
								<div class="main__number">01</div>
								<div class="main__pagination"></div>
							</div>

						</div>
					</div>
					<div class="main__letter">
						f
					</div>
				</div>
				<div class="main__right">
					<div class="main__text">
						Furniture & decor
					</div>
					<a href="#" class="main__share">
						<img src="img/main/share.png" alt="">
					</a>
					<div class="main__label label">
						<div class="label__wrapper">
							<h1 class="label__title">Wooder</h1>
							<a href="" class="label__button">
								<span></span>
								<p>learn more <span></span></p>
							</a>
							<div class="label__touch">
								<div class="label__mouse">
									<a href="">
										<img src="img/main/mouse.png" alt="">
									</a>
								</div>
								<a href="" class="label__link">
									<div class="label__arrow"></div>
								</a>
								<a href="" class="label__text">
									scroll
								</a>
							</div>
						</div>
					</div>
				</div>
			</section>
			<section class="description">
				<div class="container">
					<div class="description__body">
						<div class="description__block">
							<div class="description__wrapper">
								<div class="description__column">
									<div class="description__info info">
										<div class="info__title">
											Furniture
										</div>
										<div class="info__text">
											Wooder utilizes materials otherwise left behind, rendered useless in their original
											intent. By creating new pieces made of reclaimed barnwood
										</div>
										<a href="" class="info__button lrn">
											<p>learn more <span></span></p>
										</a>
									</div>
								</div>
								<div class="description__column description__column_img">
									<img src="img/description/01.png" alt="">
								</div>
								<div class="description__number">
									<p>01<span>.</span></p>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>
		</div>
		<!-- @import "footer.kit" -->
	</div>
	<script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
	<!-- @import "js.kit" -->
</body>

</html>